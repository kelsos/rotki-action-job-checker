    "@types/node": "16.11.56",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.25.0",
    "eslint-plugin-github": "4.4.0",
    "eslint-plugin-jest": "27.1.3",
    "jest": "28.1.3",
    "jest-circus": "28.1.3",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4"
